generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Country {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(100)
}

model Continent {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(100)
}
