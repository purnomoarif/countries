generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model countries {
  id   Int    @id
  name String @db.VarChar(100)
}

model indonesia {
  id   Int    @id
  name String @db.VarChar(100)
}

model continents {
  id   Int    @id
  name String @db.VarChar(100)
}